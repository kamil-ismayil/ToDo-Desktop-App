<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Pane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml" stylesheets="stylesheet.css" fx:controller="RegisterCustomer">
   <children>
      <Label layoutX="57.0" layoutY="63.0" text="Name" />
      <Label layoutX="57.0" layoutY="114.0" text="Adress" />
      <Label layoutX="59.0" layoutY="159.0" text="email" />
      <Label layoutX="58.0" layoutY="206.0" text="Phone" />
      <TextField fx:id="customerName" layoutX="110.0" layoutY="59.0" />
      <TextField fx:id="customerAddress" layoutX="110.0" layoutY="110.0" />
      <TextField fx:id="customerEmail" layoutX="110.0" layoutY="155.0" />
      <TextField fx:id="customerPhone" layoutX="110.0" layoutY="202.0" />
      <Button layoutX="67.0" layoutY="283.0" mnemonicParsing="false" onMouseClicked="#addCustomer" text="Add" />
      <Button layoutX="220.0" layoutY="283.0" mnemonicParsing="false" onMouseClicked="#goBackButton" text="Back" />
   </children>
</Pane>
